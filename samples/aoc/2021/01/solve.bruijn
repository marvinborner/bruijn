:import std/Math .
:import std/String .

part1 [{ [[(1 <? 0) (+1) (+0)]] | 0 , ~0 }]

part2 [{ [[(1 <? 0) (+1) (+0)]] | 0 , ~(~(~0)) }]

main [parts nums]
	nums string→number <$> (lines 0)
	parts [∑(part1 0) : ∑(part2 0)]

