:import std/String .

all-eq? [land? (zip-with eq? 0 (tail 0))]

all-gre? [land? (zip-with les? 0 (tail 0))]

# --- tests ---

list-a "abc" : ("abc" : {}("abc"))

list-b "abc" : ("def" : {}("ghi"))

:test (all-eq? list-a) ([[1]])
:test (all-eq? list-b) ([[0]])
:test (all-gre? list-a) ([[0]])
:test (all-gre? list-b) ([[1]])

main [[0]]
